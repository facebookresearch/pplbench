{
  "model": {
    "class": "robust_regression.RobustRegression",
    "args": {
      "n": 2000,
      "k": 10
    }
  },
  "iterations": 1500,
  "trials": 3,
  "ppls": [
    {
      "name": "stan",
      "inference": {
        "class": "inference.VI",
        "infer_args": {
          "algorithm": "fullrank"
        }
      },
      "legend": {
        "color": "red",
        "name": "stan-VI-full"
      }
    },
    {
      "name": "stan",
      "inference": {
        "class": "inference.MCMC"
      },
      "legend": {
        "color": "blue"
      }
    },
    {
      "name": "beanmachine",
      "inference": {
        "class": "inference.MCMC",
        "infer_args": {
          "algorithm": "GlobalNoUTurnSampler"
        }
      },
      "legend": {"color": "purple", "name": "bm-nuts"}
    }
  ],
  "save_samples": true,
  "loglevels": {
    "beanmachine": "INFO",
    "pystan": "INFO",
    "pplbench": "INFO"
  },
  "figures": {
    "generate_pll": true,
    "suffix": "png"
  }
}
